"*":
  Hydrogen:
    debug: true
    languageMappings: "{\"javascript\":\"babel es6 javascript\"}"
    startupCode: "{\"Python 3\":\"print('hello')\", \"javascript\":\"require('babel-register');require('babel-polyfill');\"}"
  Termrk:
    defaultHeight: 0
  "atom-beautify":
    css:
      align_assignments: true
      beautify_on_save: true
      end_with_newline: true
      force_indentation: true
      no_lead_zero: true
      preserve_newlines: true
      selector_separator_newline: true
    general:
      _analyticsUserId: "c606af2a-e7c4-4562-b02b-641ba8e38737"
    js:
      beautify_on_save: true
      default_beautifier: "ESLint Fixer"
    jsx:
