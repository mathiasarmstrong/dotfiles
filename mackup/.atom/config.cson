      "flow"
      "flow-ide"
      "linter-flow"
      "language-gfm"
      "markdown-preview"
      "pretty-json"
    ]
    projectHome: "/Users/jered/code/"
    reopenProjectMenuCount: 0
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "atom-dark-syntax"
    ]
    titleBar: "custom"
    useTreeSitterParsers: false
    warnOnLargeFileLimit: 20
  editor:
